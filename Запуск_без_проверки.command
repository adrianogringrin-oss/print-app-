#!/bin/bash
# Простой скрипт запуска без проверки rembg (самый безопасный способ)

cd "$(dirname "$0")"

echo "Запуск программы извлечения принтов..."
echo ""

# Запускаем программу через безопасный лаунчер
python3 safe_launch.py

# Оставляем окно открытым в случае ошибки
if [ $? -ne 0 ]; then
    echo ""
    echo "Нажмите Enter для выхода..."
    read
fi
